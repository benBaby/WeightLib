<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CornerLabelButton">
        <attr name="CLBIcon" format="reference" />
        <attr name="CLBTitle" format="string" />
        <attr name="CLBNumber" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomActionBar">
        <attr name="cabTitleName" format="string" />
        <attr name="cabTitleColor" format="color" />
        <attr name="cabTitleSize" format="dimension" />
        <attr name="cabRightTextName" format="string" />
        <attr name="cabRightTextColor" format="color" />
        <attr name="cabRightTextSize" format="dimension" />
        <attr name="cabRightTextShow" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CommonLayoutA">
        <!-- 设置标题文字 -->
        <attr name="CLAText" format="string" />
        <!-- 设置标题颜色 -->
        <attr name="CLATextColor" format="color" />
        <!-- 设置标题大小 -->
        <attr name="CLATextSize" format="dimension" />
        <!-- 设置标题文字左边的图片 -->
        <attr name="CLATextDrawableStart" format="reference" />
        <!-- 设置右边图片 -->
        <attr name="CLAImageViewSrc" format="reference" />
        <!-- 设置左边图片 -->
        <attr name="CLALeftViewSrc" format="reference" />
        <!-- 左边的图标是否显示 -->
        <attr name="CLALeftViewShow" format="boolean" />
        <!-- 右边的图标是否显示 -->
        <attr name="CLARightViewShow" format="boolean" />
        <!-- 右边的文字 -->
        <attr name="CLARightText" format="string" />
        <!-- 右边的文字大小 -->
        <attr name="CLARightTextSize" format="dimension" />
        <!-- 右边的文字颜色 -->
        <attr name="CLARightTextColor" format="color" />
        <!-- 下划线颜色 -->
        <attr name="CLADividerColor" format="color" />
        <!-- 右边可编辑视图 hint  -->
        <attr name="CLARightEditTextHint" format="string" />
        <!-- 右边可编辑视图 值 -->
        <attr name="CLARightEditTextValue" format="string" />
        <!-- 右边可编辑视图 值 颜色  -->
        <attr name="CLARightEditTextValueColor" format="color" />
        <!-- 右边可编辑视图 值 字体大小  -->
        <attr name="CLARightEditTextValueSize" format="dimension" />
        <!-- 右边可编辑视图  值 是否能修改 -->
        <attr name="CLARightEditTextEnable" format="boolean" />
        <!-- 右边 显示的是可编辑的View 还是普通的View  默认显示普通的视图 -->
        <attr name="CLARightEditTextViewShow" format="boolean" />
    </declare-styleable>
</resources>